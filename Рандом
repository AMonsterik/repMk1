#include <iostream>
#include <random>
#include <chrono>

using namespace std;

int main() {
    // Создаем генератор случайных чисел
    random_device rd; // Инициализация случайного числа
    mt19937 gen(rd()); // Генератор на основе случайного числа
    uniform_int_distribution<> distrib(1, 100); // Распределение от 1 до 100

    // Генерируем 10 случайных чисел
    for (int i = 0; i < 10; ++i) {
        int randomNumber = distrib(gen); // Генерация случайного числа
        cout << randomNumber << " "; // Вывод случайного числа
    }
    cout << endl; // Переход на новую строку
    return 0; // Завершение программы
}
